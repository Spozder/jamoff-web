    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "flatpickr": "^4.5.1",
    "jwt-simple": "^0.5.6",
    "nouislider": "^11.1.0",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
